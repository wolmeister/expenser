{
  "name": "@expenser/api",
  "author": "Victor Wolmeister",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --build",
    "dev": "ts-node-dev --respawn --transpile-only --clear -r dotenv/config src/index.ts",
    "add-migration": "knex migrate:make -x ts",
    "run-migrations": "knex migrate:latest",
    "run-last-migration": "knex migrate:up",
    "rollback-migrations": "knex migrate:rollback",
    "rollback-last-migration": "knex migrate:down"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "debug": "^4.3.1",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.10.0",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.2",
    "objection": "^2.2.14",
    "pg": "^8.5.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.34",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.2.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.3"
  }
}
